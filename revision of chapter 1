create database Simran; 
show databases; 
use Simran; 
select database(); 
create table persons(
personId int,
firstname varchar(50),
lastname varchar(50)
); 
describe persons; 
alter table persons add email varchar(100);  
create table abhays(
abhayid int primary key,
firstname varchar(40),
lastname varchar(50),
address varchar(90)
); 
describe abhays; 
alter table abhays add acount varchar(80);   



